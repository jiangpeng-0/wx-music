/* pages/home-video/index.wxss */
page{
  padding: 0 20rpx;
}
.v-inputs{
  height: 80rpx;
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  background-color: #fafafa;
  display: flex;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;
}
.v-input{
  background-color: #fff;
  border-radius: 80rpx;
  height: 60rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.icon-search{
  margin-left: 30rpx;
}
.ipt{
  flex: 1;
}
.swiper{
  width: 100%;
  height: 320rpx;
  margin-top: 80rpx;
  /* position: sticky; */
  /* top: 0; */
  background-color: #ffffff;
  /* z-index: 999; */
  border-radius: 10rpx;
  overflow: hidden;
  /* 有些机型圆角不生效 时因为手机系统webview渲染的原因  加上这行代码*/
  transform: translateY(0);
}
